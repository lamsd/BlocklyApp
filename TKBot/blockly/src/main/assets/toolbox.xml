<toolbox>

    // Module's move
    <category name="Move">
        <block type="movem1m2"></block>
        <block type="move_fbward"></block>
        <block type="servo_no"></block>
    </category>

    //Moulde's sensor
    <category name="Sensor">
        <block type="ultrasonic_port">
            <field name="Ul_Port">"02"</field>
        </block>
        <block type="temperature_tit">
            <field name="tem_port">"02"</field>
        </block>
        <block type="potenl_tit"></block>
        <block type="touch_sensor"></block>
        <block type="avoid_sensor"></block>
        <block type="line_follow2"></block>
    </category>

    <category name="Control" colour="#5CA65C">
        <block type="event_program_starts" />
        <block type="event_repeat_forever" />
        <block type="controls_repeat_ext">
            <value name="TIMES">
                <shadow type="math_number">
                    <field name="NUM">10</field>
                </shadow>
            </value>
        </block>
        <block type="wait" />
    </category>

    // Module's logic
    <category name="Logic" colour="#5C81A6">
        <block type="controls_if" />
        <block type="logic_compare" />
        <block type="logic_operation" />
        <block type="compare"></block>
        <block type="logic_negate" />
        <block type="logic_boolean_workaround" />
    </category>

    // Module's math
    <category name="Math" colour="#5C68A6">
        <block type="math_number">
            <field name="NUM">0</field>
        </block>
        <block type="math_arithmetic">
            <field name="OP">ADD</field>
            <value name="A">
                <shadow type="math_number">
                    <field name="NUM">1</field>
                </shadow>
            </value>
            <value name="B">
                <shadow type="math_number">
                    <field name="NUM">1</field>
                </shadow>
            </value>
        </block>
    </category>

    // Module's text
    <category name="Text" colour="#5CA68D">
        <block type="text">
            <field name="TEXT"></field>
        </block>
    </category>

    // Module's Variable
    <category name="Variables" colour="#5CA68D" custom="VARIABLE"/>

</toolbox>